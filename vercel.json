{
  "rewrites": [
    {
      "source": "/legal/(.*)",
      "destination": "/legal/$1"
    },
    {
      "source": "/support/(.*)",
      "destination": "/support/$1"
    },
    {
      "source": "/feedback/(.*)",
      "destination": "/feedback/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}